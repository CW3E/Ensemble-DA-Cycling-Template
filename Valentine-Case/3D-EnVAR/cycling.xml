<?xml version="1.0"?>
<!DOCTYPE workflow
[
  <!-- GSI Observation-Analysis-Forecast Cycling Workflow -->

  <!-- COMET-DEPENDENT ITEMS -->
  <!ENTITY HOME_SPACE          "/cw3e/mead/projects/cwp130/scratch/cgrudzien">
  <!-- END COMET-DEPENDENT -->

  <!-- USER-DEPENDENT ITEMS -->
  <!ENTITY EMAIL               "cgrudzien@ucsd.edu">
  <!ENTITY USERID              "cgrudzien">
  <!-- END USER-DEPENDENT -->

  <!-- WORKFLOW SETTINGS -->
  <!ENTITY PROJ_HOME           "&HOME_SPACE;/GSI-WRF-Cycling-Template/Valentine-Case/3D-EnVAR">
  <!ENTITY SCRIPTS             "&PROJ_HOME;/scripts">
  <!ENTITY DRIVERS             "&SCRIPTS;/drivers">
  <!ENTITY ENVIRONMENTS        "&SCRIPTS;/environments">
  <!ENTITY PROJ_DATA           "&PROJ_HOME;/data">
  <!ENTITY LOG                 "&PROJ_DATA;/log">
  <!ENTITY INPUT_DATAROOT      "&PROJ_DATA;/cycle_io">
  <!ENTITY STATIC_DATA         "&PROJ_DATA;/static">
  <!-- END WORKFLOW SETTINGS -->

  <!-- SOFTWARE SETTINGS -->
  <!ENTITY WPS_SERIAL_ROOT     "&HOME_SPACE;/WRF_CODE/WPS_serial">
  <!ENTITY WPS_PARALLEL_ROOT   "&HOME_SPACE;/WRF_CODE/WPS_parallel">
  <!ENTITY WRF_ROOT            "&HOME_SPACE;/WRF_CODE/WRF">
  <!ENTITY WRFDA_ROOT          "&HOME_SPACE;/WRF_CODE/WRF_DA">
  <!ENTITY GSI_ROOT            "&HOME_SPACE;/comGSIv3.7_EnKFv1.3">
  <!ENTITY CRTM_VERSION        "2.3.0">
  <!-- END SOFTWARE SETTINGS -->

  <!-- CYCLING SETTINGS -->
  <!ENTITY CYCLE_START         "201902090000">
  <!ENTITY CYCLE_END           "201902090600">
  <!ENTITY CYCLE_INTV          "06:00:00">
  <!ENTITY SECOND_CYCLE        "201902090600">
  <!ENTITY DA_MODE             "No"> <!-- Set to "No" to run WRF cycles without GSI or WRFDA analysis updates -->
  <!ENTITY COLD_START_MODE     "No"> <!-- Set to "Yes" to make every GSI analysis from real.exe outputs -->
  <!ENTITY N_ENS               "02"> <!-- Must equal the max index the "ENS" var for metatasks above-->
  <!-- END CYCLING SETTINGS -->

  <!-- UNGRIB SETTINGS -->
  <!ENTITY BKG_DATA            "GEFS"> <!-- GFS and GEFS currently supported for 3D-(En)VAR -->
  <!ENTITY UNGRIB_CONSTANT     "&ENVIRONMENTS;/WPS_constants.ksh">
  <!ENTITY IF_ECMWF_ML         "No">
  <!-- END UNGRIB SETTINGS -->

  <!-- METGRID SETTINGS -->
  <!ENTITY METGRID_CONSTANT    "&ENVIRONMENTS;/WPS_constants.ksh">
  <!-- END METGRID SETTINGS -->

  <!-- REAL SETTINGS -->
  <!ENTITY REAL_CONSTANT       "&ENVIRONMENTS;/WRF_constants.ksh">
  <!-- END REAL SETTINGS -->

  <!-- WRF SETTINGS -->
  <!ENTITY WRF_CONSTANT        "&ENVIRONMENTS;/WRF_constants.ksh">
  <!ENTITY MAX_WRF_DOM         "2">
  <!ENTITY FCST_LENGTH         "12"> <!-- This setting is applied to all WRF preprocessing and WRF -->
  <!ENTITY FCST_INTERVAL       "01">
  <!ENTITY DATA_INTERVAL       "06"> <!-- This setting is applied to all WRF preprocessing and WRF -->
  <!ENTITY IF_SST_UPDATE       "No"> <!-- This setting is applied to both WRF and real.exe runs -->
  <!ENTITY IF_FEEDBACK         "No"> <!-- This setting defines 1- or 2-way nesting with WRF domains -->
  <!ENTITY NIO_TASKS_PER_GROUP "0"> <!-- Set NIO_TASKS_PER_GROUP=0 for default TURN OFF QUILTING --> 
  <!ENTITY NIO_GROUPS          "4"> 
  <!-- END WRF SETTINGS -->

  <!-- GSI SETTINGS -->
  <!ENTITY GSI_CONSTANT        "&ENVIRONMENTS;/GSI_constants.ksh">
  <!ENTITY MAX_GSI_DOM         "2">
  <!ENTITY IF_SATRAD           "No">
  <!ENTITY IF_HYBRID           "Yes">
  <!ENTITY IF_4DENVAR          "No">
  <!ENTITY IF_NEMSIO           "No">
  <!ENTITY IF_ONEOB            "No">
  <!-- END GSI SETTINGS -->
 
  <!-- JOB SETTINGS -->
  <!ENTITY PROJECT             "cwp130">
  <!ENTITY SCHED               "slurm">
  <!ENTITY PARTITION           "compute"> 
  <!ENTITY MPIRUN              "mpirun">
  <!ENTITY UNGRIB_PROC         "1">
  <!ENTITY UNGRIB_MEM_PER_CPU  "5G">
  <!ENTITY UNGRIB_NODESIZE     "24">
  <!ENTITY UNGRIB_NODES        "1">
  <!ENTITY UNGRIB_WC           "02:00:00">
  <!ENTITY METGRID_PROC        "24">
  <!ENTITY METGRID_MEM_PER_CPU "5G">
  <!ENTITY METGRID_NODESIZE    "24">
  <!ENTITY METGRID_NODES       "2">
  <!ENTITY METGRID_WC          "02:00:00">
  <!ENTITY REAL_PROC           "24">
  <!ENTITY REAL_MEM_PER_CPU    "5G">
  <!ENTITY REAL_NODESIZE       "24">
  <!ENTITY REAL_NODES          "2">
  <!ENTITY REAL_WC             "03:00:00">
  <!ENTITY WRFDA_PROC          "1">
  <!ENTITY WRFDA_MEM_PER_CPU   "5G">
  <!ENTITY WRFDA_NODESIZE      "24">
  <!ENTITY WRFDA_NODES         "1">
  <!ENTITY WRFDA_WC            "00:45:00">
  <!ENTITY WRF_PROC            "384">
  <!ENTITY WRF_MEM_PER_CPU     "2G">
  <!ENTITY WRF_NODESIZE        "24">
  <!ENTITY WRF_NODES           "16">
  <!ENTITY WRF_WC              "12:00:00">
  <!ENTITY GSI_PROC            "352">
  <!ENTITY GSI_MEM_PER_CPU     "5G">
  <!ENTITY GSI_NODESIZE        "22">
  <!ENTITY GSI_NODES           "16">
  <!ENTITY GSI_WC              "00:45:00">
  <!-- END JOB SETTINGS -->
]>

<workflow realtime="F" scheduler="&SCHED;">
  <cycledef group="first_cycle">&CYCLE_START; &CYCLE_START; &CYCLE_INTV;</cycledef>
  <cycledef group="q_daily">&SECOND_CYCLE; &CYCLE_END; &CYCLE_INTV;</cycledef>
  <log verbosity="10"><cyclestr>&LOG;/workflow/workflow_@Y@m@d@H.log</cyclestr></log>
  
  <metatask name="ensemble_pre_proc">
    <var name="ENS_N">00 01 02</var>  
    <task name="ungrib_ens_#ENS_N#" maxtries="1" cycledefs="first_cycle,q_daily">
      <command>&DRIVERS;/ungrib.ksh</command>
      <cores>&UNGRIB_PROC;</cores>
      <nodesize>&UNGRIB_NODESIZE;</nodesize>
      <walltime>&UNGRIB_WC;</walltime>
      <jobname><cyclestr>ungrib_@Y@m@d@H</cyclestr></jobname>
      <join><cyclestr>&LOG;/ungrib/ungrib_ens_#ENS_N#_@Y@m@d@H.log</cyclestr></join>
      <native>--partition=&PARTITION;</native>
      <native>--account=&PROJECT;</native>
      <native>--export=ALL</native>
      <native>--nodes=&UNGRIB_NODES;</native>
      <native>--mem-per-cpu=&UNGRIB_MEM_PER_CPU;</native>
      <envar>
        <name>CONSTANT</name>
        <value>&UNGRIB_CONSTANT;</value>
      </envar>
      <envar>
        <name>BKG_DATA</name>
        <value>&BKG_DATA;</value>
      </envar>
      <envar>
         <name>ENS_N</name>
         <value>#ENS_N#</value>
      </envar>
      <envar>
        <name>START_TIME</name>
        <value><cyclestr>@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>BKG_START_TIME</name>
        <value><cyclestr offset="-&CYCLE_INTV;">@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>FCST_LENGTH</name>
        <value>&FCST_LENGTH;</value>
      </envar>
      <envar>
        <name>DATA_INTERVAL</name>
        <value>&DATA_INTERVAL;</value>
      </envar>
      <envar>
        <name>MAX_DOM</name>
        <value>&MAX_WRF_DOM;</value>
      </envar>
      <envar>
        <name>WPS_ROOT</name>
        <value>&WPS_SERIAL_ROOT;</value>
      </envar>
      <envar>
        <name>STATIC_DATA</name>
        <value>&STATIC_DATA;</value>
      </envar>
      <envar>
        <name>INPUT_DATAROOT</name>
        <value><cyclestr>&INPUT_DATAROOT;/@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>IF_ECMWF_ML</name>
        <value>&IF_ECMWF_ML;</value>
      </envar>
      <dependency> 
        <or>
          <streq><left><cyclestr>@Y@m@d@H@M</cyclestr></left><right>&CYCLE_START;</right></streq>
          <taskdep task="wrf_ens_#ENS_N#" cycle_offset="-&CYCLE_INTV;" state="SUCCEEDED"/>
        </or>
      </dependency>
    </task>

    <task name="metgrid_ens_#ENS_N#" maxtries="2" cycledefs="first_cycle,q_daily">
      <command>&DRIVERS;/metgrid.ksh</command>
      <cores>&METGRID_PROC;</cores>
      <nodesize>&METGRID_NODESIZE;</nodesize>
      <walltime>&METGRID_WC;</walltime>
      <jobname><cyclestr>metgrid_@Y@m@d@H</cyclestr></jobname>
      <join><cyclestr>&LOG;/metgrid/metgrid_ens_#ENS_N#_@Y@m@d@H.log</cyclestr></join>
      <native>--partition=&PARTITION;</native>
      <native>--account=&PROJECT;</native>
      <native>--export=ALL</native>
      <native>--nodes=&METGRID_NODES;</native>
      <native>--mem-per-cpu=&METGRID_MEM_PER_CPU;</native>
      <envar>
        <name>CONSTANT</name>
        <value>&METGRID_CONSTANT;</value>
      </envar>
      <envar>
        <name>START_TIME</name>
        <value><cyclestr>@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
         <name>ENS_N</name>
         <value>#ENS_N#</value>
      </envar>
      <envar>
        <name>FCST_LENGTH</name>
        <value>&FCST_LENGTH;</value>
      </envar>
      <envar>
        <name>DATA_INTERVAL</name>
        <value>&DATA_INTERVAL;</value>
      </envar>
      <envar>
        <name>MAX_DOM</name>
        <value>&MAX_WRF_DOM;</value>
      </envar>
      <envar>
        <name>WPS_ROOT</name>
        <value>&WPS_PARALLEL_ROOT;</value>
      </envar>
      <envar>
        <name>STATIC_DATA</name>
        <value>&STATIC_DATA;</value>
      </envar>
      <envar>
        <name>INPUT_DATAROOT</name>
        <value><cyclestr>&INPUT_DATAROOT;/@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>MPIRUN</name>
        <value>&MPIRUN;</value>
      </envar>
      <dependency> 
        <taskdep task="ungrib_ens_#ENS_N#" state="SUCCEEDED"/>
      </dependency>
    </task>

    <task name="real_ens_#ENS_N#" maxtries="2" cycledefs="first_cycle,q_daily">
      <command>&DRIVERS;/real.ksh</command>
      <cores>&REAL_PROC;</cores>
      <nodesize>&REAL_NODESIZE;</nodesize>
      <walltime>&REAL_WC;</walltime>
      <jobname><cyclestr>real_@Y@m@d@H</cyclestr></jobname>
      <join><cyclestr>&LOG;/real/real_ens_#ENS_N#_@Y@m@d@H.log</cyclestr></join>
      <native>--partition=&PARTITION;</native>
      <native>--account=&PROJECT;</native>
      <native>--export=ALL</native>
      <native>--nodes=&REAL_NODES;</native>
      <native>--mem-per-cpu=&REAL_MEM_PER_CPU;</native>
      <envar>
        <name>CONSTANT</name>
        <value>&REAL_CONSTANT;</value>
      </envar>
      <envar>
         <name>ENS_N</name>
         <value>#ENS_N#</value>
      </envar>
      <envar>
        <name>START_TIME</name>
        <value><cyclestr>@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>FCST_LENGTH</name>
        <value>&FCST_LENGTH;</value>
      </envar>
      <envar>
        <name>DATA_INTERVAL</name>
        <value>&DATA_INTERVAL;</value>
      </envar>
      <envar>
        <name>MAX_DOM</name>
        <value>&MAX_WRF_DOM;</value>
      </envar>
      <envar>
        <name>WRF_ROOT</name>
        <value>&WRF_ROOT;</value>
      </envar>
      <envar>
        <name>STATIC_DATA</name>
        <value>&STATIC_DATA;</value>
      </envar>
      <envar>
        <name>INPUT_DATAROOT</name>
        <value><cyclestr>&INPUT_DATAROOT;/@Y@m@d@H</cyclestr></value>
      </envar>
      <envar>
        <name>IF_SST_UPDATE</name>
        <value>&IF_SST_UPDATE;</value>
      </envar>
      <envar>
        <name>MPIRUN</name>
        <value>&MPIRUN;</value>
      </envar>
      <envar>
        <name>REAL_PROC</name>
        <value>&REAL_PROC;</value>
      </envar>
      <dependency> 
        <taskdep task="metgrid_ens_#ENS_N#" state="SUCCEEDED"/>
      </dependency>
    </task>
  </metatask>

  <task name="wrfda_lowbc" maxtries="1" cycledefs="q_daily">
    <command>&DRIVERS;/wrfda.ksh</command>
    <cores>&WRFDA_PROC;</cores>
    <nodesize>&WRFDA_NODESIZE;</nodesize>
    <walltime>&WRFDA_WC;</walltime>
    <jobname><cyclestr>wrfda_lowbc_@Y@m@d@H</cyclestr></jobname>
    <join><cyclestr>&LOG;/wrfda/wrfda_lowbc_@Y@m@d@H.log</cyclestr></join>
    <native>--partition=&PARTITION;</native>
    <native>--account=&PROJECT;</native>
    <native>--export=ALL</native>
    <native>--nodes=&WRFDA_NODES;</native>
    <native>--mem-per-cpu=&WRFDA_MEM_PER_CPU;</native>
    <native>--mail-type BEGIN</native>
    <native>--mail-type END</native>
    <native>--mail-type=FAIL</native>
    <native>--mail-user=&EMAIL;</native>
    <envar>
      <name>CONSTANT</name>
      <value>&WRF_CONSTANT;</value>
    </envar>
    <envar>
      <name>N_ENS</name>
      <value>&N_ENS;</value>
    </envar>
    <envar>
      <name>IF_LOWER</name>
      <value>Yes</value>
    </envar>
    <envar>
      <name>ANAL_TIME</name>
      <value><cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>MAX_DOM</name>
      <value>&MAX_WRF_DOM;</value>
    </envar>
    <envar>
      <name>WRFDA_ROOT</name>
      <value>&WRFDA_ROOT;</value>
    </envar>
    <envar>
      <name>STATIC_DATA</name>
      <value>&STATIC_DATA;</value>
    </envar>
    <envar>
      <name>INPUT_DATAROOT</name>
      <value><cyclestr>&INPUT_DATAROOT;/@Y@m@d@H</cyclestr></value>
    </envar>
    <dependency> 
      <and>
        <metataskdep metatask="ensemble_pre_proc"/>
	<streq><left>&DA_MODE;</left><right>Yes</right></streq>
      </and>
    </dependency>
  </task>

  <task name="gsi_cold_start_yes" maxtries="1" cycledefs="first_cycle">
    <command>&DRIVERS;/gsi.ksh</command>
    <nodesize>&GSI_NODESIZE;</nodesize>
    <cores>&GSI_PROC;</cores>
    <walltime>&GSI_WC;</walltime>
    <jobname><cyclestr>gsi_@Y@m@d@H</cyclestr></jobname>
    <join><cyclestr>&LOG;/gsi/gsi_@Y@m@d@H.log</cyclestr></join>
    <native>--partition=&PARTITION;</native>
    <native>--account=&PROJECT;</native>
    <native>--export=ALL</native>
    <native>--nodes=&GSI_NODES;</native>
    <native>--mem-per-cpu=&GSI_MEM_PER_CPU;</native>
    <native>--mail-type BEGIN</native>
    <native>--mail-type END</native>
    <native>--mail-type FAIL</native>
    <envar>
      <name>CONSTANT</name>
      <value>&GSI_CONSTANT;</value>
    </envar>
    <envar>
      <name>MAX_DOM</name>
      <value>&MAX_GSI_DOM;</value>
    </envar>
    <envar>
      <name>ANAL_TIME</name>
      <value><cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>IF_COLD_START</name>
      <value>Yes</value>
    </envar>
    <envar>
      <name>IF_SATRAD</name>
      <value>&IF_SATRAD;</value>
    </envar>
    <envar>
      <name>IF_OBSERVER</name>
      <value>No</value>
    </envar>
    <envar>
      <name>N_ENS</name>
      <value>&N_ENS;</value>
    </envar>
    <envar>
      <name>IF_HYBRID</name>
      <value>&IF_HYBRID;</value>
    </envar>
    <envar>
      <name>N_ENS</name>
      <value>&N_ENS;</value>
    </envar>
    <envar>
      <name>IF_4DENVAR</name>
      <value>&IF_4DENVAR;</value>
    </envar>
    <envar>
      <name>IF_NEMSIO</name>
      <value>&IF_NEMSIO;</value>
    </envar>
    <envar>
      <name>IF_ONEOB</name>
      <value>&IF_ONEOB;</value>
    </envar>
    <envar>
      <name>STATIC_DATA</name>
      <value>&STATIC_DATA;</value>
    </envar>
    <envar>
      <name>INPUT_DATAROOT</name>
      <value>&INPUT_DATAROOT;/<cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>GSI_ROOT</name>
      <value>&GSI_ROOT;</value>
    </envar>
    <envar>
      <name>CRTM_VERSION</name>
      <value>&CRTM_VERSION;</value>
    </envar>
    <envar>
      <name>MPIRUN</name>
      <value>&MPIRUN;</value>
    </envar>
    <dependency> 
      <and>
        <metataskdep metatask="ensemble_pre_proc"/> 
        <streq><left>&DA_MODE;</left><right>Yes</right></streq>
      </and>
    </dependency>
  </task>

  <task name="gsi_cold_start_&COLD_START_MODE;" maxtries="1" cycledefs="q_daily">
    <command>&DRIVERS;/gsi.ksh</command>
    <nodesize>&GSI_NODESIZE;</nodesize>
    <cores>&GSI_PROC;</cores>
    <walltime>&GSI_WC;</walltime>
    <jobname><cyclestr>gsi_@Y@m@d@H</cyclestr></jobname>
    <join><cyclestr>&LOG;/gsi/gsi_@Y@m@d@H.log</cyclestr></join>
    <native>--partition=&PARTITION;</native>
    <native>--account=&PROJECT;</native>
    <native>--export=ALL</native>
    <native>--nodes=&GSI_NODES;</native>
    <native>--mem-per-cpu=&GSI_MEM_PER_CPU;</native>
    <native>--mail-type BEGIN</native>
    <native>--mail-type END</native>
    <native>--mail-type FAIL</native>
    <envar>
      <name>CONSTANT</name>
      <value>&GSI_CONSTANT;</value>
    </envar>
    <envar>
      <name>MAX_DOM</name>
      <value>&MAX_GSI_DOM;</value>
    </envar>
    <envar>
      <name>ANAL_TIME</name>
      <value><cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>IF_COLD_START</name>
      <value>&COLD_START_MODE;</value>
    </envar>
    <envar>
      <name>IF_SATRAD</name>
      <value>&IF_SATRAD;</value>
    </envar>
    <envar>
      <name>IF_OBSERVER</name>
      <value>No</value>
    </envar>
    <envar>
      <name>N_ENS</name>
      <value>&N_ENS;</value>
    </envar>
    <envar>
      <name>IF_HYBRID</name>
      <value>&IF_HYBRID;</value>
    </envar>
    <envar>
      <name>IF_4DENVAR</name>
      <value>&IF_4DENVAR;</value>
    </envar>
    <envar>
      <name>IF_NEMSIO</name>
      <value>&IF_NEMSIO;</value>
    </envar>
    <envar>
      <name>IF_ONEOB</name>
      <value>&IF_ONEOB;</value>
    </envar>
    <envar>
      <name>STATIC_DATA</name>
      <value>&STATIC_DATA;</value>
    </envar>
    <envar>
      <name>INPUT_DATAROOT</name>
      <value>&INPUT_DATAROOT;/<cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>GSI_ROOT</name>
      <value>&GSI_ROOT;</value>
    </envar>
    <envar>
      <name>CRTM_VERSION</name>
      <value>&CRTM_VERSION;</value>
    </envar>
    <envar>
      <name>MPIRUN</name>
      <value>&MPIRUN;</value>
    </envar>
    <dependency> 
      <or>
        <taskdep task="wrfda_lowbc" state="SUCCEEDED"/> 
        <and>
          <metataskdep metatask="ensemble_pre_proc"/> 
          <streq><left>&COLD_START_MODE;</left><right>Yes</right></streq>
        </and>
      </or>
    </dependency>
  </task>

  <task name="wrfda_latbc" maxtries="1" cycledefs="first_cycle,q_daily">
    <command>&DRIVERS;/wrfda.ksh</command>
    <cores>&WRFDA_PROC;</cores>
    <nodesize>&WRFDA_NODESIZE;</nodesize>
    <walltime>&WRFDA_WC;</walltime>
    <jobname><cyclestr>wrfda_latbc_@Y@m@d@H</cyclestr></jobname>
    <join><cyclestr>&LOG;/wrfda/wrfda_latbc_@Y@m@d@H.log</cyclestr></join>
    <native>--partition=&PARTITION;</native>
    <native>--account=&PROJECT;</native>
    <native>--export=ALL</native>
    <native>--nodes=&WRFDA_NODES;</native>
    <native>--mem-per-cpu=&WRFDA_MEM_PER_CPU;</native>
    <native>--mail-type BEGIN</native>
    <native>--mail-type END</native>
    <native>--mail-type=FAIL</native>
    <native>--mail-user=&EMAIL;</native>
    <envar>
      <name>CONSTANT</name>
      <value>&WRF_CONSTANT;</value>
    </envar>
    <envar>
      <name>N_ENS</name>
      <value>&N_ENS;</value>
    </envar>
    <envar>
      <name>IF_LOWER</name>
      <value>No</value>
    </envar>
    <envar>
      <name>ANAL_TIME</name>
      <value><cyclestr>@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>MAX_DOM</name>
      <value>&MAX_WRF_DOM;</value>
    </envar>
    <envar>
      <name>WRFDA_ROOT</name>
      <value>&WRFDA_ROOT;</value>
    </envar>
    <envar>
      <name>STATIC_DATA</name>
      <value>&STATIC_DATA;</value>
    </envar>
    <envar>
      <name>INPUT_DATAROOT</name>
      <value><cyclestr>&INPUT_DATAROOT;/@Y@m@d@H</cyclestr></value>
    </envar>
    <envar>
      <name>MPIRUN</name>
      <value>&MPIRUN;</value>
    </envar>
